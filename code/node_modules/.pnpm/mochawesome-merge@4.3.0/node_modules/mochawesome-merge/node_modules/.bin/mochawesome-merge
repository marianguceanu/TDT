#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules/mochawesome-merge/bin/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules/mochawesome-merge/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules/mochawesome-merge/bin/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules/mochawesome-merge/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/mochawesome-merge@4.3.0/node_modules:/home/marianguceanu/Documents/Code/TDT/code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mochawesome-merge.js" "$@"
else
  exec node  "$basedir/../../bin/mochawesome-merge.js" "$@"
fi
